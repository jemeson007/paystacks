
<p><div class="row">
   <div class="col-md-3 col-md-offset-1">
     <strong>Supplier Name :</strong>
   </div>
   <div class="col-md-8">
     <%= @finance.supplierName %>
   </div>
  </div>
</p>
<p>
  <div class="row">
    <div class="col-md-3 col-md-offset-1">
      <strong>Email :<strong>
    </div>
      <div class="col-md-8">
        <%= @finance.email %>
      </div>
  </div>
</p>
<p>
  <div class="row">
    <div class="col-md-3 col-md-offset-1">
      <strong>Ingredient</strong>
    </div>
    <div class="col-md-8">
      <%= @finance.Ingredient %>
    </div>
  </div>
</p>

<p>
  <div class="row">
    <div class="col-md-3 col-md-offset-1">
